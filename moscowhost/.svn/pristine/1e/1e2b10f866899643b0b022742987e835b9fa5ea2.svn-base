		{{form.media}}
	

    
<style>
/*paging*/

.paging {
	margin: -2px 0 0;
	border:none;
}
.paging ul {
	margin: 0;
	padding: 0;
	list-style: none;
	font-size: 12px;
	font-weight: 700;
	text-align: center;
}
.paging li {
	display: inline-block;
	margin: 0 2px;
	vertical-align: middle;
	color: #535353;
}
.paging .start a,
.paging .prev a,
.paging .next a,
.paging .end a {
	position: relative;
	top: 2px;
	display: block;
	width: 17px;
	height: 18px;
	background: url(/media/images/sprite_private.png) no-repeat;
	text-indent: -999em;
	text-decoration: none;
	border:none;
}
.paging .start {
	margin-right: 5px;
	border:none;
}
.paging .start a {
	background-position: 0 -63px;
	width:5px;
	border:none;
}
.paging .prev {
	margin-right: 4px;
	border:none;
}
.paging .prev a {
	width: 3px;
	background-position: -18px -63px;
	border:none;
}
.paging .next {
	margin-left: 4px;
	border:none;
}
.paging .next a {
	width:3px;
	background-position: -33px -63px;
	border:none;
}
.paging .end {
	margin-left: 5px;
	border:none;
}
.paging .end a {
	background-position: -48px -63px;
	border:none;
}
.paging .text {
	width: 32px;
	margin: 0 3px;
	padding: 5px 8px;
	background: #fff;
	box-shadow: inset 0 1px 0 rgba(0,0,0,.06);	
	border:none;
	color: #535353;
	font: 14px Arial, sans-serif;
	text-align: center;
	
}
</style>
{% endblock %}

