        RETURN OLD;
    ELSIF (TG_OP = 'UPDATE') THEN
        IF (NEW.""" + PHONETRANSACTION_FIELD_NAME + """) THEN
            UPDATE billservice_account SET ballance=COALESCE(ballance, 0)+OLD.summ_record_out WHERE id=OLD.account_id;
            UPDATE billservice_account SET ballance=COALESCE(ballance, 0)+OLD.summ+OLD.summ_record_in WHERE id=OLD.called_account_id;
            UPDATE billservice_account SET ballance=COALESCE(ballance, 0)-NEW.summ_record_out WHERE id=NEW.account_id;
            UPDATE billservice_account SET ballance=COALESCE(ballance, 0)-NEW.summ-NEW.summ_record_in WHERE id=NEW.called_account_id;
        ELSE
            UPDATE billservice_account SET ballance=COALESCE(ballance, 0)+OLD.summ+OLD.summ_record_out WHERE id=OLD.account_id;
            UPDATE billservice_account SET ballance=COALESCE(ballance, 0)-NEW.summ-NEW.summ_record_out WHERE id=NEW.account_id;
            IF (NEW.called_account_id NOTNULL) THEN
                UPDATE billservice_account SET ballance=COALESCE(ballance, 0)+OLD.summ_record_in WHERE id=OLD.called_account_id;
                UPDATE billservice_account SET ballance=COALESCE(ballance, 0)-NEW.summ_record_in WHERE id=NEW.called_account_id;
            END IF;
        END IF;
        RETURN NEW;
    END IF;
    RETURN NULL;
END;
$BODY$
LANGUAGE plpgsql;
"""

