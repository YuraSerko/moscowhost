  }
  .header:after {
    clear: both;
    display: table;
    content: ''; }
  .header .logo {
    float: left;
    margin: 0 70px 0 0;
    padding: 48px 0 0; }
    .header .logo a {
      display: block; }
    .header .logo img {
      vertical-align: middle; }
  .header .user {
 
    margin: 0;
    padding: 0;
    list-style: none;
    float: left;
    width: 289px;
    padding: 68px 0 0; }
    .header .user li {
      position: relative; }
    .header .user i {
      position: absolute;
      top: 1px;
      left: -20px;
      display: block; }
    .header .user a:hover {
      color: #fff; }
    .header .user a:after {
      position: absolute;
      top: 0;
      left: -20px;
      width: 21px;
      height: 16px;
      content: ''; }
  .header .user-login span {
    position: relative;
    overflow: hidden;
    display: inline-block;
    max-width: 130px;
    text-overflow: ellipsis;
    white-space: nowrap; }
  .header .links {
    float: left;
    width: 153px;
    margin: 0 40px 0 0;
    padding: 68px 0 0;
    list-style: none; }
    .header .links li {
      position: relative; }
    .header .links i {
      position: absolute;
      top: 1px;
      left: -22px;
      display: block; }
    .header .links a {
      color: #fff; }
      .header .links a:after {
        position: absolute;
        top: 0;
        left: -20px;
        width: 21px;
        height: 16px;
        content: ''; }
    .header .links span {
      position: relative;
      display: block;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap; }
  .header .im {
    margin: 0;
    padding: 0;
    list-style: none;
    float: left;
    padding: 68px 0 0;
    max-width: 120px; }
    .header .im li {
      position: relative; }
    .header .im i {
      position: absolute;
      top: 0;
      left: -25px;
      display: block; }
    .header .im div {
      position: relative;
      /*overflow: hidden;*/
      text-overflow: ellipsis;
      white-space: nowrap; }
  .header .phones {
    margin: 0;
	margin-right: -240px;
    padding: 0;
    list-style: none;
    float: right;
    padding: 68px 0 0;
    max-width: 210px; }
    .header .phones li {
      position: relative; }
    .header .phones i {
      position: absolute;
      top: 1px;
      left: -23px;
      display: block; }
    .header .phones small {
      color: #ec9633;
      font-size: 10px; }
    .header .phones div {
      position: relative;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap; }
  .header .nav {
    clear: both;
    margin-left: -12px;
    padding: 27px 0 0;
    font-size: 20px;
    font-weight: 700; }
    .header .nav ul {
      margin: 0;
      padding: 0;
      list-style: none;
      white-space: nowrap; }
      .header .nav ul:after {
        clear: both;
        display: table;
        content: ''; }
      .header .nav ul ul {
        position: absolute;
        top: 100%;
        left: 0;
        display: none;
        padding: 0 0 8px;
        background: #810000;
        box-shadow: 10px 10px 0 rgba(35, 0, 0, 0.4);
        font-size: 14px;
        font-weight: 400; }
        .header .nav ul ul ul {
          top: 0;
          left: 100%;
          margin-left: -9px;
          padding: 6px 0 13px 12px; }
          .header .nav ul ul ul:before {
            position: absolute;
            top: 0;
            left: 0;
            width: 9px;
            height: 32px;
            background: #810000;
            border: solid rgba(35, 0, 0, 0.4);
            border-width: 0 5px 5px 0;
            content: ''; }
    .header .nav li {
      position: relative;
      float: left;
      margin-right: 18px; }
      .header .nav li.nav-drop .arr {
        position: absolute;
        top: 15px;
        right: 6px;
        width: 0;
        height: 0;
        border-style: solid;
        border-width: 4px 4px 0;
        border-color: #fff transparent transparent;
        content: ''; }
    .header .nav ul ul li.nav-drop .arr {
      position: absolute;
      top: 9px;
      right: 15px;
      width: 0;
      height: 0;
      border-style: solid;
      border-width: 4px 0 4px 4px;
      border-color: transparent transparent transparent #fff;
      content: ''; }
    .header .nav li:hover > ul {
      display: block; }
    .header .nav ul ul li {
      float: none;
      margin: 0; }
      .header .nav ul ul li:first-child {
        margin: 0; }
    .header .nav a {
      display: block;
      text-decoration: none; }
      .header .nav a:after {
        position: absolute;
        top: 6px;
        right: 0;
        width: 20px;
        height: 20px;
        content: ''; }
      .header .nav a span {
        display: block;
        padding: 4px 19px 6px 12px;
        text-decoration: underline; }
    .header .nav li a:hover span {
      color: #fff; }
    .header .nav li.nav-drop:hover span {
	/*Ширина меню подсветки*/
	width: 90%;
      background-color: #810000;
      background-image: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #d71818), color-stop(100%, #600606));
      background-image: -webkit-linear-gradient(top, #d71818 0%, #600606 100%);
      background-image: -moz-linear-gradient(top, #d71818 0%, #600606 100%);
      background-image: -o-linear-gradient(top, #d71818 0%, #600606 100%);
      background-image: linear-gradient(top, #d71818 0%, #600606 100%);
      box-shadow: 10px 10px 0 rgba(35, 0, 0, 0.4);
      color: #fff;
      text-decoration: none; }
    .header .nav ul ul a {
     padding: 4px 12px;
      text-decoration: underline; }
    .header .nav ul ul li:hover > a {
      color: #fff;
      text-decoration: none; }

