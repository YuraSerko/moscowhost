/*====================================================================*/
.login-form {
  position: relative; }

.register-form:after,
.login-form:after {
  position: absolute;
  top: 0;
  left: 0;
  display: block;
  width: 1px;
  height: 100%;
  background: #810000;
  content: ''; }

.register-form:after {
  left: 270px; }

.popup-form-captcha img {
  margin-right: 2px;
  vertical-align: middle;
  border: 1px solid #bbb; }

.popup-form-captcha .text {
  vertical-align: middle;
  width: 76px;
  padding: 5px;
  font-size: 18px; }

.popup-form-forgot {
  margin: 0 25px 4px -11px;
  padding: 7px 9px 0;
  border: 2px solid transparent; }


.popup-form-forgot-opened {
  background: #ffe491;
  background: rgba(255, 228, 145, 0.5);
  border-color: #aa0018;
  border-radius: 5px; }

.popup-form-forgot-h,
.popup-form-forgot-h:hover {
  border-bottom: 1px dotted;
  color: #6b0614;
  font-weight: 700;
  text-decoration: none; }

.popup-form-forgot-i {
  display: none;
  padding: 11px 10px 10px; }

.popup-form-forgot-i h4 {
  margin: 0 0 2px;
  color: #6b0614;
  font-size: 12px; }

.popup-form-forgot-i p {
  margin: 0 0 2px;
  color: #767676;
  font-size: 11px;
  line-height: 13px; }

.popup-form-forgot-i .text {
  width: 182px; }

.popup-form-forgot-i .submit {
  margin-top: 3px;
  padding: 3px 9px; }
  
