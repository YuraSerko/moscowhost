/*--------------------------------------------------------*/
/* tables */
.type-1 {
	width: 100%;
	margin: 0 0 30px;
	border: solid #bebebe;
	border-width: 0 1px 1px;
	border-spacing: 0;
	border-radius: 0 0 5px 5px;
	
	color: #535353;
	font-size: 12px;
}

.type-1 caption {
	padding: 5px 17px 6px;
	background: #B41F1F;
	border: solid #810000;
	border-width: 1px 1px 0;
	border-radius: 5px 5px 0 0;
	color: #ffffff;
	font-size: 14px;
	font-weight: 700;
	text-align: left;
}

.type-1 th {
	padding: 8px 17px;
           background: #ffffff;
		   
	font-weight: 700;
	text-align: left;
}

.type-1 td {
	padding: 6px 17px 5px;
	 background: #ffffff;
	text-align: left;
}

.type-1 .odd th,
.type-1 .odd td {
	background: #f3f3f3;
}

.type-1 thead + tbody th,
.type-1 thead + tbody td {
	background: #fff;
}

.type-1 thead + tbody .odd th,
.type-1 thead + tbody .odd td {
	background: #fafafa;
}

.type-1 tbody tr:last-child td:first-child,
.type-dl tbody tr:last-child th {
	border-radius: 0 0 0 5px;
}

.type-1 tbody tr:last-child td:last-child,
.type-dl tbody tr:last-child td {
	border-radius: 0 0 5px;
}

.type-1 .sep {
	display: inline-block;
	vertical-align: middle;
	margin: 0 4px;
}

.type-1 a {
	text-decoration: none;
}

.type-1 a:hover {
	text-decoration: underline;
}

.type-dl td {
	text-align: right;
}

.type-2 {
	width: 800px;
	margin: 20px;
	margin-left: -10px;
	border-spacing: 0;
	border-radius: 0 0 5px 5px;
	color: #2a2a2a;
	font-size: 12px;
}

.type-2 th {
	padding: 9px 19px 10px;
	background: #B41F1F;
	border: solid #810000;
	border-width: 1px 0 0;
	color: #fff;
	text-align: left;
}

.type-2 th:first-child {
	border-width: 1px 0 0 1px;
	border-radius: 5px 0 0;
}

.type-2 th:last-child {
	border-width: 1px 1px 0 0;
	border-radius: 0 5px 0 0;
}

.type-bth th {
	padding: 6px 19px 5px;
	font-size: 14px;
}
.type-2 td {
    background: none repeat scroll 0 0 #Ffffff;
    padding: 3px 19px 4px;
    text-align: left;
}


.type-2 td:first-child {
	border-left: 1px solid #810000;
}

.type-2 td:last-child {
	border-right: 1px solid #810000;
}

.type-2 tbody tr:last-child td {
	border-bottom: 1px solid #810000;
}

.type-2 tbody tr:last-child td:first-child {
	border-radius: 0 0 0 5px;
}

.type-2 tbody tr:last-child td:last-child {
	border-radius: 0 0 5px;
}

.type-bth td {
	padding: 5px 19px 6px;
}

.type-2 .odd td {
	background: #E7E7E7;
}

.type-1 .center,
.type-2 .center {
	text-align: center;
}

.type-3 {
	width: 100%;
	margin: 23px 0 30px;
	border-spacing: 0;
	border-radius: 0 0 5px 5px;
	color: #2a2a2a;
	font-size: 12px;
}

.type-3 caption {
	padding: 0 0 7px;
	font-size: 14px;
	font-weight: 700;
	text-align: center;
}

.type-3 th {
	padding: 5px 19px;
	background: #810000;
	border: 1px solid #ffffff;
	color: #fff;
	text-align: left;
}

.type-3 th:first-child {
	border-radius: 5px 0 0;
}

.type-3 th:last-child {
	border-radius: 0 5px 0 0;
}

.type-3 th.colspan {
	padding-top: 7px;
	background: #970000;
	border-width: 1px 1px 0 0;
	text-align: center;
}

.type-3 th.span {
	border-radius: 0;
	border-width: 1px 1px 1px 0;
}

.type-3 td {
	padding: 3px 19px 4px;
	background: #fff;
	border-right: 1px solid #810000;
	text-align: left;
}

.type-3 td:first-child {
	border-left: 1px solid #810000;
}

.type-3 tbody tr:last-child td {
	border-bottom: 1px solid #810000 ;
}

.type-3 .odd td {
	background: #fafafa;
}

.type-3 tbody tr:last-child td:first-child {
	border-radius: 0 0 0 5px;
}

.type-3 tbody tr:last-child td:last-child {
	border-radius: 0 0 5px;
}

