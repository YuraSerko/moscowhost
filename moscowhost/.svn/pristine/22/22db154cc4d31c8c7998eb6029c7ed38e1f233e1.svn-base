/*аккордеон для главной страницы*/
/*начало аккордеона */
	.ac-container{
	    height: 580px;
	    margin: 0 auto 30px auto;
	}
	
	.ac-container>div>label{
		border: 1px solid #34495e;
		border-right: 0px;
	    width: 46px;
	    float: left;
	    position: relative;
	    /*z-index: 20;*/ /*закоментировал для всплывающего окна*/
	    display: inline-block;
	    height: 598px;
	    cursor: pointer;
	    color: #fff;
	    font-size: 19px;
	    background: #0084ff;
	    line-height: 598px;
	}

	.ac-container>div>input[type='radio'] {
    	position: absolute;
    	left: -9999px;
    	opacity: 0;
	}

	.ac-container .article{
	    border: 1px solid #34495e;
	    border-right: 0;
	    border-left: 0; 
	    background: #fff;
	    overflow: hidden;
	    height: 598px;
	    width: 0;
	    float: left;
	    position: relative;
	    z-index: 10;
	    transition: all 0.2s linear;
	    -ms-transition: all 0.2s linear;
	}

	.ac-container input:checked ~ .article {
	    transition: all 0.2s linear;
	    width: 882px;
	}
/* окончание аккордеона */	





