/* в принципе общая для обоих файлов верхнее меню и тд.  !!!!окончание*/
/**************************************************************************IE lte 8.0***************************************************************************/
#tab_1left.checked ~ .tabs_contLeft #tab_cc1 {
	position: static;
	left: 0;
	height: 70px;
}

#tab_2left.checked ~ .tabs_contLeft #tab_cc2 {
	position: static;
	left: 0;
	height: 190px;
}

#tab_3left.checked ~ .tabs_contLeft #tab_cc3 {
	position: static;
	left: 0;
	height: 60px;
}

#tab_4left.checked ~ .tabs_contLeft #tab_cc4 {
	position: static;
	left: 0;
	height: 130px;
}

#tab_5left.checked ~ .tabs_contLeft #tab_cc5 {
	position: static;
	left: 0;
	height: 110px;
	
}

#tab_6left.checked ~ .tabs_contLeft #tab_cc6 {
	position: static;
	left: 0;
	height: 110px;
	border-bottom: 1px solid #34495e;
}

#tab_6left.checked  ~ #tab_l6left {
	border-bottom: 0px;
}

#tab_l010.checked  ~ .tabs_cont>#tab_c1,
#tab_l020.checked  ~ .tabs_cont>#tab_c2,
#tab_l030.checked  ~ .tabs_cont>#tab_c3,
#tab_l040.checked  ~ .tabs_cont>#tab_c4,
#tab_l050.checked  ~ .tabs_cont>#tab_c5,
#tab_l060.checked  ~ .tabs_cont>#tab_c6 {
	position: static;
	left: 0;
	opacity: 1;
	display: block;
}

#tab_4.checked  ~ .tab_cont_m1,
#tab_5.checked  ~ .tab_cont_m2 {
	display: block;
}

#tab_4.checked  ~ #tab_l11>.btnn>img,
#tab_5.checked  ~ #tab_l12>.btnn>img {
	filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=2);
}
