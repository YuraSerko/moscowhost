/* ================================POP_UP STILES====================================================== */
/* Popup */

.tint {
	position: fixed;
	top: 0;
	left: 0;
	display: none;
	z-index: 500;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,.45);
}

.popup {
	overflow: hidden;
    position: fixed;
    top: 30%;
	left: 50%;
	display: none;
	z-index: 600;
	width: 858px;
	background: #fff;
	box-shadow: 0 0 5px rgba(20,52,89,.75), 0 0 46px rgba(23,110,215,.75);
	border-radius: 3px;	

	margin-left:-429px;
	margin-top:-100px;
}

.popup-close {
	position: absolute;
	top: 6px;
	right: 6px;
	display: block;
	width: 21px;
	height: 21px;
	/*background: url(/media/pictures/sprite_public.png) no-repeat -62px 0;*/
	cursor: pointer;
}

.popup .icon-close {
  position: absolute;
  top: 15px;
  right: 15px;
  z-index: 160;
  display: block;
  cursor: pointer;
}

