.acc_main {
	position: relative;
	float: left;
	margin: 0 0 0 8px;
	height: 725px;
	width: 790px;
	background-color: #fff;
	padding: 10px;
	border: 1px solid #34495e;
	overflow:auto; /* прокрутка */
}

.acc_mainmenu {
	margin: 20px 0 0 0;
}

.tabs .menuacc {
	display: block;
	float: left;
	cursor: pointer;
	position: relative;
	width: 200px;
	height: 150px;
	text-align: center;
}

.tabs_acc {
	display: inline-block;
    position: relative;
    margin-top: 18px;
    height: 500px;
}

.tabs_acc .tabs_cont {
	position: relative;
	bottom: 0;
	width: 790px;
	overflow-y: auto;
}

.tab_cont_m1,
.tab_cont_m2 {
	display: none;
	margin: 5px;
}

#tab_4:checked  ~ .tab_cont_m1,
#tab_5:checked  ~ .tab_cont_m2 {
	display: block;
}

#tab_4:checked  ~ #tab_l11>.btnn>img,
#tab_5:checked  ~ #tab_l12>.btnn>img {
	-webkit-transform: rotate(-180deg);
    -moz-transform: rotate(-180deg);
    -o-transform: rotate(-180deg);
    -ms-transform: rotate(-180deg);
	transform: rotate(180deg);
}

.tabs_menu {
	position: relative;
	bottom: 0;
	overflow: hidden;
}

.btnn {
	height: 30px;
	line-height: 30px;
	text-align: center;
	background-color: #0084ff;
	color: #fff;
	width: 190px;
	margin: 5px;
}


