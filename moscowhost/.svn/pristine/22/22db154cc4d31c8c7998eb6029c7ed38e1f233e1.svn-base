
* {
	margin: 0;
	padding: 0;
}

body {
	background-color: #f6f6f6;
	font-family: Ubuntu, Arial;
}

label {
	cursor: pointer;
}

img {
	border: none;
	outline: none;
}

.wrap {
	position: relative;
	width: 1024px;
	margin: auto;
}

.tabs {
	display: inline-block;
    position: relative;
    margin: 0 0 0 50px;
}

.tabs .top {
	display: block;
	float: left;
	background: #ffffff;
	color: #414141;
	cursor: pointer;
	font-size: 13px;
	position: relative;
	width: 165px;
	height: 190px;
	text-align: center;
	z-index: 1;
	margin-right: 40px;
}

.tabs .inp {
	display: block;
}

.tabs_cont > div {
	display: none;
	top: 0;
	opacity: 0;
	transition: opacity .1s ease-in;
}

#tab_1:checked  ~ #tab_l1,
#tab_2:checked  ~ #tab_l2,
#tab_3:checked  ~ #tab_l3,
#tab_4:checked  ~ #tab_l4 {
	color: #0084ff;
}

#tab_1:checked ~ .tabs_cont #tab_c1,
#tab_2:checked ~ .tabs_cont #tab_c2,
#tab_3:checked ~ .tabs_cont #tab_c3,
#tab_4:checked ~ .tabs_cont #tab_c4 {
	position: static;
	left: 0;
	opacity: 1;
	display: block;
}

