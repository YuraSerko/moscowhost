{% extends 'account/profile.html' %}
{% load i18n form_tags lib_tags %}

{% block extrahead %}
    {{ block.super }} 
    <link rel="stylesheet" type="text/css" href="{{ MEDIA_URL }}css/interface-style1.css" />
{% endblock %}

{% block content %}
{% if userservice %}
      <h4>{% trans "Activated services:" %}</h4>
      <table>
            <thead>
                <tr>
                    <th style="width: 25%;">{% trans "Service" %}</th>
                    <th style="width: 15%;">{% trans "initial_fee" %}</th>
                    <th style="width: 15%;">{% trans "abonent_fee" %}</th>
                    <th style="width: 25%;">{% trans "Delete" %}</th>
                </tr>
            </thead>
            <tbody>
              
              
               {% for userse in userservice %}
            
                    <tr>
                        <td><a href="../../../change/" target="_blank">{{ userse.name }}</a></td>
                        <td>{% if userse.initial_fee %}{{ userse.initial_fee.value }}{% else %}---{% endif %}</td>
                        <td>{% if userse.abonent_fee %}{{ userse.abonent_fee.value }}{% else %}---{% endif %}</td>
                     
                         <td><a class="delete_link" href="delete/{{ userse.id }}/">{% trans "Delete" %}</a></td>
                        
                    </tr>
             
                {% endfor %}
                
               {% else %}
                <h4><p class="warning"> нет активных услуг!</p></h4>  
                {%endif%}
                
            </tbody>
        </table>
    
    
{% endblock %}

